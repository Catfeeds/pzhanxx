.container {
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  z-index:101;
}

.container-fixed {
  position: fixed;
  top:0;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  z-index:101;
  
}

.status-bar {
  width: 100%;
}

.bar {
  display: flex;
  flex-direction: row;
  width: 100%;
  background: #fff;
  align-items: center;
  justify-content: center;
  padding:10rpx 20rpx;
  box-sizing: border-box;
}

.bar  .back{
  display: flex;
  flex-direction: row;
  font-size: 30rpx;
  position: absolute;
  left: 20rpx;
  width: 40rpx;
  height: 40rpx;
  
}

.bar  .back image{
  width: 100%;
  height: 100%;
}

.bar .tab {
  display: flex;
  flex-direction: row;
  font-size: 35rpx;
  font-weight: 500;
  box-sizing: border-box;  
  
}

.choose{
  font-weight: 500;
  color: #303133;
  border-bottom: 2px #409EFF solid;
}