.report {
  background-color: rgba(0,0,0, .3);
  height:100%;
  width: 100%;
  position: fixed;
  top:0;
  z-index:100;
}


.report .list {
  position: fixed;
  bottom: 0;
  /* height: 480rpx; */
  width: 100%;
  background-color: #f1f1f2;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  text-align: center;
  font-size:30rpx;
  color:#606266;
  
}


.report .list .operate {

}

.report .list .operate view {
  height:90rpx;
  line-height: 90rpx;
  border-top: 1px solid #F2F6FC;
  background-color: #fff;
  
}

.report .list .cancel {
  margin-top:20rpx;
  height:90;
  line-height: 90rpx;
  border-top: 1px solid #F2F6FC;
  background-color: #fff;
}
